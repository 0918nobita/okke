{
  "name": "okke",
  "description": "Type-safe validation with Branded types and precisely inferred error types",
  "keywords": ["typescript", "validation"],
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./str": {
      "require": "./dist/str.cjs",
      "import": "./dist/str.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "lint": "biome lint",
    "format": "biome format"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "tsup": "8.3.5",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  }
}
